    <script>
      document.addEventListener("DOMContentLoaded", () => {
        let isUserScrolling = false;
        let contentLoaded = false;
        let skipScrollToTop = false;
        const scrollToTopLinks = document.querySelectorAll(".link-el");
        const dynamicContent = document.getElementById("dynamic-content");
        const faqContent = document.getElementById("faq-content");
        const loadContent = () => {
          if (!contentLoaded) {
            // faqContent.innerHTML = ``;
            contentLoaded = true;
            const faqItems = document.querySelectorAll(
              ".desti-durable-faq details"
            );

            // Add tabindex to each question for keyboard focus
            faqItems.forEach((item, index) => {
              item.setAttribute("tabindex", "0");
              item.addEventListener("keydown", (event) => {
                if (event.key === "Enter" || event.key === " ") {
                  event.preventDefault();
                  item.open = !item.open;
                }
              });
            });

            // Allow keyboard navigation with arrow keys
            document.addEventListener("keydown", (event) => {
              const focusedElement = document.activeElement;
              if (
                !focusedElement ||
                !focusedElement.closest(".desti-durable-faq")
              )
                return;

              const currentIndex = Array.from(faqItems).indexOf(focusedElement);
              if (event.key === "ArrowDown") {
                event.preventDefault();
                const nextItem = faqItems[currentIndex + 1] || faqItems[0];
                nextItem.focus();
              } else if (event.key === "ArrowUp") {
                event.preventDefault();
                const prevItem =
                  faqItems[currentIndex - 1] || faqItems[faqItems.length - 1];
                prevItem.focus();
              }
            });
          }
        };

        const handleScrollByWheel = (event) => {
          if (event.deltaY !== 0) {
            isUserScrolling = true;
            dynamicContent.style.display = "block";
            loadContent();
            console.log("Scrolling triggered by user");
          } else {
            isUserScrolling = false;
            // dynamicContent.style.display = 'none';
            console.log("Scrolling triggered by code");
          }
        };

        const handleCheckScrollMobile = () => {
          const width = window.innerWidth;
          if (width <= 1024) {
            isUserScrolling = true;
            dynamicContent.style.display = "block";
            loadContent();
          }
        };

        const handleScrollbarClick = (e) => {
          const target = e.target;

          if (
            e.offsetX > target.clientWidth ||
            e.offsetY > target.clientHeight
          ) {
            isUserScrolling = true;
            dynamicContent.style.display = "block";
            loadContent();
          }
        };

        // Trigger actions when Tab or Arrow keys are pressed
        document.addEventListener("keydown", (event) => {
          if (event.key === "Tab") {
            dynamicContent.style.display = "block";
            loadContent();
          }
          // if ( event.key === 'ArrowDown' || event.key === 'ArrowUp') {
          //     setTimeout(() => {
          //       dynamicContent.style.display = 'block';
          //       loadContent();
          //     },2500)
          // }
        });

        scrollToTopLinks.forEach((link) => {
          link.addEventListener("click", (event) => {
            skipScrollToTop = true; // Mark that we don't want to scroll to top
            event.preventDefault(); // Prevent default link behavior
            const url = link.getAttribute("href");
            window.location.href = url; // Navigate to the link's href
            setTimeout(() => {
              skipScrollToTop = false;
            }, 1500);
          });
        });

        window.addEventListener("wheel", handleScrollByWheel);
        window.addEventListener("scroll", handleCheckScrollMobile);
        window.addEventListener("mousedown", handleScrollbarClick);

        // Clean up event listeners when the page is unloaded
        window.addEventListener("beforeunload", () => {
          window.removeEventListener("wheel", handleScrollByWheel);
          window.removeEventListener("scroll", handleCheckScrollMobile);
          window.removeEventListener("mousedown", handleScrollbarClick);
        });

        window.addEventListener("beforeunload", () => {
          window.scrollTo(0, 0);
        });
      });
    </script>
